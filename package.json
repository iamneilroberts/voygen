{
  "name": "voygen",
  "version": "1.0.0",
  "description": "Voygen - AI-Powered Travel Assistant with LibreChat Integration",
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "continue:test": "continue test-models && continue test-mcp-servers",
    "deploy:staging": "npm run build && wrangler deploy --env staging",
    "deploy:production": "npm run build && wrangler deploy --env production"
  },
  "keywords": [
    "ai",
    "travel",
    "mcp",
    "llm-agnostic",
    "continue-dev",
    "travel-planning"
  ],
  "author": "Voygen Team",
  "license": "MIT",
  "dependencies": {
    "@continue/cli": "^0.9.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "axios": "^1.5.0",
    "date-fns": "^2.30.0",
    "chart.js": "^4.3.0",
    "react-chartjs-2": "^5.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "tailwindcss": "^3.3.0",
    "zod": "^3.22.0",
    "joi": "^17.9.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vitest": "^0.34.0",
    "@vitest/ui": "^0.34.0",
    "c8": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voygen/voygen.git"
  },
  "bugs": {
    "url": "https://github.com/voygen/voygen/issues"
  },
  "homepage": "https://voygen.app"
}