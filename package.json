{
  "name": "voygen",
  "version": "1.0.0",
  "description": "Voygen - AI-Powered Travel Assistant with LibreChat Integration",
  "main": "src/index.ts",
  "scripts": {
    "dev": "npm --prefix voygen-api run dev",
    "build": "npm --prefix voygen-api ci || npm --prefix voygen-api install && npm --prefix voygen-api run build && npm --prefix librechat-source ci && npm --prefix librechat-source run frontend",
    "start": "node voygen-api/dist/index-with-librechat.js",
    "preview": "npm run build && node voygen-api/dist/index-with-librechat.js",
    "test": "cd librechat-source && npm test",
    "test:mcp": "cd mcp-local-servers/mcp-chrome && npm test",
    "test:unit": "npm run test:mcp",
    "test:integration": "echo 'Integration tests not implemented yet'",
    "test:e2e": "echo 'E2E tests not implemented yet'",
    "lint": "cd librechat-source && npm run lint",
    "lint:fix": "cd librechat-source && npm run lint:fix",
    "format": "cd librechat-source && npm run format",
    "install-deps": "cd librechat-source && npm install && cd ../mcp-local-servers/mcp-chrome && npm install && npm run build",
    "setup": "./scripts/setup.sh",
    "setup:hooks": "node scripts/setup-hooks.js",
    "deploy:mcp": "cd remote-mcp-servers/d1-database-improved && npm run deploy && cd ../prompt-instructions-d1-mcp && npm run deploy && cd ../github-mcp-cta && npm run deploy",
    "handoff": "node scripts/handoff.js",
    "handoff:done": "node scripts/handoff.js --update-current",
    "changelog:add": "node scripts/changelog-add.js"
  },
  "keywords": [
    "ai",
    "travel",
    "mcp",
    "llm-agnostic",
    "continue-dev",
    "travel-planning"
  ],
  "author": "Voygen Team",
  "license": "MIT",
  "dependencies": {
    "@continue/cli": "^0.9.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "axios": "^1.5.0",
    "date-fns": "^2.30.0",
    "chart.js": "^4.3.0",
    "react-chartjs-2": "^5.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "tailwindcss": "^3.3.0",
    "zod": "^3.22.0",
    "joi": "^17.9.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vitest": "^0.34.0",
    "@vitest/ui": "^0.34.0",
    "c8": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voygen/voygen.git"
  },
  "bugs": {
    "url": "https://github.com/voygen/voygen/issues"
  },
  "homepage": "https://voygen.app"
}
